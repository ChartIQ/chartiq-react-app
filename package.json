{
	"name": "chartiq-react-app",
	"version": "8.7.0",
	"description": "ChartIQ's full featured charting application written in ReactJS",
	"main": "index.js",
	"scripts": {
		"build": "webpack --config=webpack.config.js --env.production",
		"start": "npm run watch -- --host=127.0.0.1 --disable-host-check=true",
		"format": "prettier --write src/**/*.{js,jsx} react-components/**/*.{js,jsx} tests/**/*.js --config .prettierrc",
		"watch": "webpack-dev-server --inline --config=webpack.config.js --watch=true"
	},
	"keywords": [
		"react",
		"charts",
		"charting",
		"chartiq",
		"component",
		"library"
	],
	"author": "ChartIQ",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.14.8",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-react": "^7.14.5",
		"@open-wc/webpack-import-meta-loader": "^0.4.7",
		"babel-loader": "^8.2.2",
		"copy-webpack-plugin": "^6.4.1",
		"css-loader": "^5.2.7",
		"extract-css-chunks-webpack-plugin": "^4.9.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^4.5.2",
		"node-sass": "^6.0.1",
		"prettier": "2.5.1",
		"resolve-url-loader": "^3.1.4",
		"sass-loader": "^10.2.0",
		"url-loader": "^4.1.1",
		"webpack": "^4.46.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.2"
	},
	"dependencies": {
		"@babel/preset-env": "^7.14.8",
		"@chartiq/react-components": "file:./react-components",
		"chartiq": "file:deps/chartiq.tgz",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-router-dom": "^5.2.0"
	}
}
