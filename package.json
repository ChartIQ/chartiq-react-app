{
	"name": "chartiq-react-app",
	"version": "8.8.0",
	"description": "ChartIQ's full featured charting application written in ReactJS",
	"main": "index.js",
	"scripts": {
		"build": "webpack --config=webpack.config.js",
		"build:prod": "webpack --config=webpack.config.js --env production",
		"start": "webpack serve -- --host=127.0.0.1 --allowed-hosts all",
		"format": "prettier --write src/**/*.{js,jsx} react-components/**/*.{js,jsx} tests/**/*.js --config .prettierrc",
		"watch": "webpack watch --config=webpack.config.js"
	},
	"keywords": [
		"react",
		"charts",
		"charting",
		"chartiq",
		"component",
		"library"
	],
	"author": "ChartIQ",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.14.8",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-react": "^7.14.5",
		"babel-loader": "^8.2.2",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.1",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.6.1",
		"prettier": "2.5.1",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.11.1"
	},
	"dependencies": {
		"@babel/preset-env": "^7.14.8",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-router-dom": "^5.3.4"
	}
}
