{
  "name": "chartiq-react-app",
  "version": "1.1.0",
  "description": "ChartIQ's full featured charting application written in ReactJS",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config=webpack.config.js --env.production",
    "build:chartiq": "sh build-scripts/build-chartiq.sh",
    "build:test": "sh build-scripts/build-test.sh",
    "build:clone-new-tests": "sh build-scripts/build-pull-new-tests.sh",
    "build:test-new": "sh build-scripts/copy-e2e-new.sh",
    "start": "npm run watch -- --host=127.0.0.1 --disable-host-check=true",
    "test": "wdio",
    "test-new": "wdio wdio-chrome.conf.js",
    "test:debug": "DEBUG=true wdio",
    "test:debug:spec": "DEBUG=true wdio --spec",
    "test:spec": "wdio --spec",
    "test:watch": "wdio --watch",
    "watch": "webpack-dev-server --inline --config=webpack.config.js --watch=true",
    "report": "allure generate ./allure-results --clean && allure open",
    "report-ci": "allure generate allure-results --clean -o allure-report"
  },
  "keywords": [
    "react",
    "charts",
    "charting",
    "chartiq",
    "component",
    "library"
  ],
  "author": "ChartIQ",
  "license": "ISC",
  "devDependencies": {
    "@actions/core": "^1.2.6",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.10.1",
    "@wdio/allure-reporter": "6.7.2",
    "@wdio/cli": "6.7.2",
    "@wdio/devtools-service": "^6.10.11",
    "@wdio/local-runner": "6.7.2",
    "@wdio/mocha-framework": "6.7.0",
    "@wdio/selenium-standalone-service": "6.10.4",
    "@wdio/spec-reporter": "6.7.0",
    "allure-commandline": "^2.13.0",
    "babel-loader": "^8.0.4",
    "babel-register": "^6.26.0",
    "bignumber.js": "^9.0.0",
    "chai": "^4.2.0",
    "chromedriver": "^87.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^3.0.0",
    "eslint-plugin-decorator-position": "^2.2.16",
    "extract-css-chunks-webpack-plugin": "^4.5.5",
    "faker": "^4.1.0",
    "file-loader": "^4.0.0",
    "form-data": "^3.0.0",
    "geckodriver": "^1.21.1",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.13.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "npm": "^6.14.5",
    "prettier": "^2.0.5",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^7.1.0",
    "wdio-chromedriver-service": "^6.0.4",
    "wdio-geckodriver-service": "^1.1.1",
    "wdio-json-reporter": "^2.0.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1",
    "zip-a-folder": "0.0.12"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@chartiq/ui-tests": "file:chartiq-ui-tests-1.0.0.tgz",
    "chartiq": "file:chartiq-8.1.0.tgz",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^5.2.0"
  }
}
