{
  "name": "chartiq-react-app",
  "version": "1.1.0",
  "description": "ChartIQ's full featured charting application written in ReactJS",
  "main": "index.js",
  "workspaces": [
    "src/chartiq-react-components"
  ],
  "scripts": {
    "build": "webpack --config=webpack.config.js --env.production",
    "build:chartiq": "sh build-scripts/build-chartiq.sh",
    "build:components": "npm run build:dev --workspace=src/chartiq-react-components",
    "build:test": "sh build-scripts/build-test.sh",
    "build:stx-symlink": "sh build-scripts/build-stx-symlink.sh",
    "check-specs": "CI=true node -e 'require(\"./build-scripts/specsChecker.js\").specsChecker()'",
    "ciq-prep-app": "sh ./build-scripts/clear-dist.sh && npm run build && node ./build-scripts/webserver-symlinks.js",
    "publish:components": "npm pack --workspace=src/chartiq-react-components",
    "start": "npm run watch -- --host=127.0.0.1 --disable-host-check=true",
    "test-chrome": "wdio wdio-chrome.conf.js --mochaOpts.grep '@NoReact' --mochaOpts.invert=true",
    "test-chrome-ci": "CI=true wdio wdio-chrome.conf.js --mochaOpts.grep '@NoReact' --mochaOpts.invert=true",
    "test-chrome:debug": "DEBUG=true wdio wdio-chrome.conf.js --mochaOpts.grep '@NoReact' --mochaOpts.invert=true",
    "test-chrome:spec": "wdio wdio-chrome.conf.js --mochaOpts.grep '@NoReact' --mochaOpts.invert=true --spec",
    "test-chrome:debug:spec": "DEBUG=true wdio wdio-chrome.conf.js --mochaOpts.grep '@NoReact' --mochaOpts.invert=true --spec",
    "test-ff": "wdio wdio-ff.conf.js --mochaOpts.grep '@NoReact' --mochaOpts.invert=true",
    "test-ff-ci": "CI=true wdio wdio-ff.conf.js --mochaOpts.grep '@NoReact' --mochaOpts.invert=true",
    "test-ff:debug": "DEBUG=true wdio wdio-ff.conf.js --mochaOpts.grep '@NoReact' --mochaOpts.invert=true",
    "test-ff:spec": "wdio wdio-ff.conf.js --mochaOpts.grep '@NoReact' --mochaOpts.invert=true --spec",
    "test-ff:debug:spec": "DEBUG=true wdio wdio-ff.conf.js --mochaOpts.grep '@NoReact' --mochaOpts.invert=true --spec",
    "watch": "webpack-dev-server --inline --config=webpack.config.js --watch=true",
    "report": "allure generate ./allure-results --clean && allure open",
    "report-ci": "allure generate allure-results --clean -o allure-report"
  },
  "keywords": [
    "react",
    "charts",
    "charting",
    "chartiq",
    "component",
    "library"
  ],
  "author": "ChartIQ",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-react": "^7.14.5",
    "@open-wc/webpack-import-meta-loader": "^0.4.7",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.2.7",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "node-sass": "^6.0.1",
    "resolve-url-loader": "^3.1.4",
    "sass-loader": "^10.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@chartiq/chartiq-react-components": "^8.3.0",
    "@babel/preset-env": "^7.14.8",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^5.2.0"
  }
}
