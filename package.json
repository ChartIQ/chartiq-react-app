{
  "name": "chartiq-react-app",
  "version": "0.0.1",
  "description": "ChartIQ's full featured charting application written in ReactJS",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config=webpack.config.js --env.production",
    "build:polyfill": "webpack --config=webpack.config.js --env.production=polyfill",
    "start": "npm run watch",
    "test": "wdio",
    "test:debug": "DEBUG=true wdio",
    "test:debug:spec": "DEBUG=true wdio --spec",
    "test:spec": "wdio --spec",
    "test:watch": "wdio --watch",
    "watch": "webpack-dev-server --inline --config=webpack.config.js --watch=true"
  },
  "keywords": [
    "react",
    "charts",
    "charting",
    "chartiq",
    "component",
    "library"
  ],
  "author": "ChartIQ",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-react": "^7.0.0",
    "@chartiq/ui-tests": "file:chartiq-ui-tests-1.0.0.tgz",
    "babel-loader": "^8.0.4",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
