{
	"name": "chartiq-react-app",
	"version": "10.1.0",
	"description": "ChartIQ's full featured charting application written in ReactJS",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"build": "webpack --config=webpack.config.js",
		"build:prod": "webpack --config=webpack.config.js --env production",
		"start": "webpack serve -- --host=127.0.0.1 --allowed-hosts all",
		"format": "prettier --write src/**/*.{js,jsx} tests/**/*.js --config .prettierrc",
		"watch": "webpack watch --config=webpack.config.js"
	},
	"keywords": [
		"react",
		"charts",
		"charting",
		"chartiq",
		"component",
		"library"
	],
	"author": "ChartIQ",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.26.4",
		"@babel/core": "^7.26.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-react": "^7.26.3",
		"babel-loader": "^9.2.1",
		"copy-webpack-plugin": "^12.0.2",
		"css-loader": "^7.1.2",
		"html-webpack-plugin": "^5.6.3",
		"mini-css-extract-plugin": "^2.9.2",
		"prettier": "3.4.2",
		"webpack": "^5.97.1",
		"webpack-cli": "^6.0.1",
		"webpack-dev-server": "^5.2.0"
	},
	"dependencies": {
		"@babel/preset-env": "^7.26.0",
		"lodash": "^4.17.23",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.1.1"
	},
	"chartiq-dependencies-for-npm": {
		"@chartiq/active-trader": "~10.1.0",
		"@chartiq/core": "~10.1.0",
		"@chartiq/technical-analysis": "~10.1.0",
		"@chartiq/web-components": "~10.1.0",
		"@chartiq/chart2music": "~10.1.0"
	}
}
