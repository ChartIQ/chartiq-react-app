{
	"name": "chartiq-react-app",
	"version": "9.6.2",
	"chartiqVersion": "9.6.2",
	"description": "ChartIQ's full featured charting application written in ReactJS",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"build": "webpack --config=webpack.config.js",
		"build:prod": "webpack --config=webpack.config.js --env production",
		"start": "webpack serve -- --host=127.0.0.1 --allowed-hosts all",
		"format": "prettier --write src/**/*.{js,jsx} react-components/**/*.{js,jsx} tests/**/*.js --config .prettierrc",
		"watch": "webpack watch --config=webpack.config.js"
	},
	"keywords": [
		"react",
		"charts",
		"charting",
		"chartiq",
		"component",
		"library"
	],
	"author": "ChartIQ",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.26.4",
		"@babel/core": "^7.26.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-react": "^7.26.3",
		"babel-loader": "^8.4.1",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.11.0",
		"html-webpack-plugin": "^5.6.3",
		"mini-css-extract-plugin": "^2.9.2",
		"prettier": "2.5.1",
		"webpack": "^5.97.1",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.15.2"
	},
	"dependencies": {
		"@babel/preset-env": "^7.23.8",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-router-dom": "^5.3.4"
	},
	"chartiq-dependencies-for-npm": {
		"@chartiq/active-trader": "^9.6.0",
		"@chartiq/core": "^9.6.0",
		"@chartiq/react-components": "^9.1.3",
		"@chartiq/technical-analysis": "^9.6.0",
		"@chartiq/web-components": "^9.6.0"
	}
}
