{
  "name": "@chartiq/react-components",
  "version": "9.9.0",
  "description": "React Components for ChartIQ library",
  "main": "index.js",
  "files": [
    "Chart/",
    "ActiveTrader/",
    "CrossSection",
    "MultiChart",
    "src/"
  ],
  "scripts": {
    "build": "babel src -d . --copy-files --source-maps inline",
    "build:prod": "babel src -d . --copy-files",
    "clean": "rm -rf Chart/ Advanced/ styles/ ActiveTrader/ CrossSection/ node_modules/",
    "watch": "babel src -d . -w --copy-files --source-maps inline",
    "prepublish": "npm run build:prod"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "keywords": [
    "component",
    "react",
    "charts",
    "chartiq"
  ],
  "author": "ChartIQ",
  "license": "ISC",
  "repository": "https://github.com/ChartIQ/chartiq-react-app",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3"
  },
  "dependencies": {}
}
