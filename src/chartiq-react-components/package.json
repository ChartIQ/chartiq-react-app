{
  "name": "@chartiq/chartiq-react-components",
  "version": "8.4.0-beta.3",
  "description": "React Components for ChartIQ library",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "babel ./ -d dist/ --ignore node_modules,dist,addCSSFiles.sh --source-maps inline",
    "postbuild": "./addCSSfiles.sh",
    "prepare": "npm run build"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "keywords": [
    "component",
    "react",
    "charts",
    "chartiq"
  ],
  "author": "ChartIQ",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.0.0"
  },
  "dependencies": {}
}
